public static void main(String[] args) {

		byte byteValue = 10;
		int intValue = byteValue; // 자동 타입 변환
		
		long longValue = 5000000000L;
		float floatValue = longValue;		// 5.0e9f로 저장
		double doubleValue = longValue;		// 5.0e9f로 저장
		
		char charValue = 'A';
		int int_Value = charValue; // 65 저장
		
	}
