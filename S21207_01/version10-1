public static void main(String[] args) {
		
		int intX = 1;
		int intY = 1;
		
		int intXnum = ++intX + 10;
		int intYnum = intY++ + 10;
		
		System.out.println("++intXnum:" + intXnum + " intX:" + intX);
		System.out.println("intYnum++:" + intYnum + " intY:" + intY);
		
		
		byte byte1 = 1;
		byte byte2 = 1;
		int byte3 = byte1 + byte2;
	}
